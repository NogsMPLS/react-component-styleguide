{
  "name": "react-component-styleguide",
  "version": "0.0.1",
  "description": "Just annotated your actual components and make your docs from that with 1 command.",
  "main": "lib/rcs.js",
  "bin": {
    "rcs": "bin/rcs"
  },
  "scripts": {
    "test": "standard && mocha test/**/*.js",
    "start": "cross-env NODE_ENV=development node bin/rcs 'example/components/**/*.js' -v -d",
    "clean": "rimraf rcs-tmp/*",
    "styleguide": "node bin/rcs 'example/components/**/*.js' -v",
    "ghpages": "npm run styleguide -- -r 'react-styleguide-generator-alt' && gh-pages -d styleguide"
  },
  "files": [
    ".babelrc",
    "app",
    "bin",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NogsMPLS/react-component-styleguide.git"
  },
  "keywords": [
    "component",
    "documentation",
    "documentor",
    "docs",
    "generator",
    "markdown",
    "react",
    "styleguide",
    "react-docgen",
    "style-guide",
    "webpack",
    "sample",
    "examples",
    "guide"
  ],
  "author": "natsmith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NogsMPLS/react-component-styleguide/issues"
  },
  "homepage": "https://github.com/NogsMPLS/react-component-styleguide#readme",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "example"
    ]
  },
  "dependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "^6.6.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-polyfill": "^6.6.0",
    "babel-register": "^6.6.0",
    "bunyan": "^1.4.0",
    "codemirror": "^5.6.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "cssnext": "^1.7.1",
    "cssnext-loader": "^1.0.1",
    "deepmerge": "anthonator/deepmerge#feature/array-options",
    "ecology": "NogsMPLS/ecology#feature/componentDidUpdate",
    "express": "^4.13.3",
    "fs-extra": "^0.24.0",
    "glob": "^7.0.0",
    "highlight.js": "^9.0.0",
    "immutable": "^3.7.5",
    "marked": "^0.3.3",
    "minimist": "^1.1.1",
    "mustache": "^2.1.2",
    "object-assign": "^4.0.1",
    "page": "^1.6.4",
    "postcss-cssnext": "2.4.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "0.8.1",
    "rekuire": "^0.1.9",
    "raw-loader": "^0.5.1",
    "react-bootstrap": "0.27.1",
    "react-docgen": "NogsMPLS/react-docgen#feature/inferDisplayName",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "2.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "slash": "^1.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "eslint-plugin-react": "^3.10.0",
    "gh-pages": "^0.4.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.0",
    "standard": "^5.4.1",
    "uglify-js": "^2.4.23"
  }
}