{
  "name": "react-component-styleguide",
  "version": "0.0.5",
  "description": "Just annotated your actual components and make your docs from that with 1 command.",
  "main": "lib/rcs.js",
  "bin": {
    "rcs": "bin/rcs"
  },
  "scripts": {
    "test": "standard && mocha test/**/*.js",
    "start": "cross-env NODE_ENV=development node bin/rcs 'example/components/**/*.js' -v -d",
    "clean": "rimraf rcs-tmp/*",
    "styleguide": "node bin/rcs 'example/components/**/*.js' -v"
  },
  "files": [
    ".babelrc",
    "app",
    "bin",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NogsMPLS/react-component-styleguide.git"
  },
  "keywords": [
    "component",
    "documentation",
    "documentor",
    "docs",
    "generator",
    "markdown",
    "react",
    "styleguide",
    "react-docgen",
    "style-guide",
    "webpack",
    "sample",
    "examples",
    "guide"
  ],
  "author": "natsmith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NogsMPLS/react-component-styleguide/issues"
  },
  "homepage": "https://github.com/NogsMPLS/react-component-styleguide#readme",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "example"
    ]
  },
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-polyfill": "^6.7.2",
    "babel-register": "^6.6.5",
    "bunyan": "^1.8.0",
    "codemirror": "^5.12.0",
    "component-playground": "^1.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cssnext": "^1.8.4",
    "cssnext-loader": "^1.0.1",
    "deepmerge": "anthonator/deepmerge#feature/array-options",
    "express": "^4.13.4",
    "fs-extra": "^0.26.7",
    "glob": "^7.0.3",
    "highlight.js": "^9.2.0",
    "immutable": "^3.7.6",
    "marked": "^0.3.5",
    "minimist": "^1.2.0",
    "mustache": "^2.2.1",
    "object-assign": "^4.0.1",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "react-bootstrap": "0.28.3",
    "react-docgen": "NogsMPLS/react-docgen#feature/inferDisplayName",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "slash": "^1.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "eslint-plugin-react": "^4.2.3",
    "gh-pages": "^0.11.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "standard": "^6.0.8",
    "uglify-js": "^2.6.2"
  }
}